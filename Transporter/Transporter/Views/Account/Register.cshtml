@model CreateAdminVm
<h1 class="text-info">Register</h1>
<form asp-controller="Account" asp-action="Register" method="post" role="form">
    <h4>Create a new acount</h4>
    <hr />
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="EmailAddress" class="col-md-2"></label>
        <div class="col-md-10">
            <input asp-for="EmailAddress" class="form-control" />
            <span asp-validation-for="EmailAddress" class="text-danger"></span>

        </div>
    </div>



    <div class="form-group">
        <label asp-for="password" type="password" class="col-md-2"></label>
        <div class="col-md-10">
            <input asp-for="password" class="form-control" />
            <span asp-validation-for="password" class="text-danger"></span>

        </div>
    </div>

    <div class="form-group">
        <div class="col-md-2">
            <button type="submit" class="btn btn-success form-control">Register</button>
        </div>

    </div>
</form>

