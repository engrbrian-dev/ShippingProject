

    <br />
    <br />
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="text-black-50">
                            <h4 class="title">Shipments</h4>
                        </div>
                        <div class="content table-responsive table-full-width">
                            <table class="table table-hover table-striped">
                                <thead>
                                    <tr>
                                        <th>Transaction Id</th>
                                        <th>Customer Name</th>
                                        <th>Customer PhoneNo</th>
                                        <th>Receiver Name</th>
                                        <th>Receiver PhoneNo</th>
                                        <th>Sender Address</th>
                                        <th>Receiver Address</th>
                                        <th>Transaction Date</th>
                                        <th>Tracking Number</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var ShippingInfo in Model)
                                    {
                                        <tr>
                                            <td>@ShippingInfo.TransactionId</td>
                                            <td>@ShippingInfo.CustomerName</td>
                                            <td>@ShippingInfo.CustomerPhoneNo</td>
                                            <td>@ShippingInfo.ReceiverName</td>
                                            <td>@ShippingInfo.ReceiverPhoneNo</td>
                                            <td>@ShippingInfo.SenderAddress</td>
                                            <td>@ShippingInfo.ReceiverAddress</td>
                                            <td>@ShippingInfo.TransactionDate</td>
                                            <td>@ShippingInfo.TrackingNumber</td>
                                            <td>
                                                <a asp-controller="Home"
                                                   asp-action="Edit"
                                                   asp-route-id="@ShippingInfo.TransactionId"
                                                   class="btn btn-info btn-fill">Edit</a>
                                            </td>
                                            <td>
                                                <a asp-controller="Home"
                                                   asp-action="DeleteStudent"
                                                   asp-route-id="@ShippingInfo.TransactionId"
                                                   class="btn btn-danger btn-fill">Delete</a>
                                        </tr>
                                    }
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
